#include<iostream>
using namespace std;

int main() {
	int a;
	char ch;
	cout<<"Enter any type of character: ";
	cin>>ch;
	a=ch;
	if(a<91 && a>64){
		cout<<"The character is in Uppercase"<<endl;
	}
	else if(a>96 && a<123){
		cout<<"The character is in Lowercase."<<endl;
	}
	else{
		cout<<"This is a Symbol."<<endl;
	}
}
